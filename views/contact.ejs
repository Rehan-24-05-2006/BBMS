  /* General All */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

/* Body Style */
body {
    font-family: Arial, sans-serif;
    line-height: 1.6;
    color: #fff;
    background-color: white;
}

/* Header Section */
header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20px;
    background-color: #33414a;
    position: fixed;
    width: 100%;
    z-index: 1000;
}

header .logo {
    font-size: 24px;
    color: #fff;
    font-weight: bold;

}

header .logo span {
    color: red;
}

header nav ul {
    list-style: none;
    display: flex;
    gap: 15px;
}

header nav ul li a {
    text-decoration: none;
    color: #fff;
    padding: 8px 12px;
    /* transition: 0.7s; */
}

header nav ul li a:hover {
    background-color: #000000;
    border-radius: 5px;
    
}

/* main */

main {
    height: 35rem;
    background: url('https://www.netbloodbank.com/images/search-back.jpg')no-repeat;
}




/* footer */
footer {
    background-color: #2d2d2d; 
    color: #fff; 
    padding: 40px 20px;
    font-family: Arial, sans-serif;
    display: flex;
    justify-content: center;
    align-items: flex-start;
    flex-wrap: wrap;
    height: 25rem;
  }
  
  .footer-content {
    width: 100%;
    /* max-width: 1200px; */
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    /* gap: 80px; */
    opacity: 0.9;
  }
  
.footer-content > div ul a {
    color: #fff;
    text-decoration: none;
    font-size: 14px;
}

.footer-content > div ul li {
    list-style: none;
    line-height: 2;
}

.important-links {
    padding-left: 10px;
}

.footer-content > div {
    margin-left: 80px;
}


.contact-info p {
    font-size: 14px;
    line-height: 2;
  }
  
  
.footer-content > div ul li a:hover {
    text-decoration: underline;
    color: #ff6347;
  }
  
.social-media a {
    color: #fff;
    text-decoration: none;
    font-size: 14px;
    margin-right: 20px;
  }
  
.social-media a:hover {
    color: #ff6347; 
  }
  
.emergency-contact p {
    font-size: 14px;
    font-weight: bold;
    color: #ff6347; 
    display: flex;
    justify-content: center;
    align-items: center;
  }



#sub-footer-id {
    margin: auto;
}

#sub-footer-id  #icons-id{
    
    display: flex;
    justify-content: center;
    align-items: center;
 

}

#sub-footer-id #icons-id > i {
    margin: 3.5rem 0 1.5rem 0.7rem;
    align-items: center;
    cursor: grab;
}

#sub-footer-id #icons-id i:hover {
    size: 5rem;
    opacity: 1;
}

.copyright {
    display: flex;
    justify-content: center;
    align-items: center;
}


