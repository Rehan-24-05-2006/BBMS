.contact-section {
  background: url('https://www.netbloodbank.com/images/bwcity.jpg') no-repeat center center/cover;
  color: white;
  text-align: center;
  padding: 60px 20px;
}

.contact-section h1 {
  font-size: 28px;
  margin-bottom: 40px;
  font-weight: bold;
  margin-top: 50px;
}

.contact-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 40px;
}

.contact-box {
  background: rgba(0, 0, 0, 0.5);
  padding: 30px 20px;
  border-radius: 15px;
  max-width: 300px;
  width: 100%;
}

.contact-box h3 {
  font-size: 20px;
  margin-bottom: 10px;
  color: #fff;
}

.contact-box p {
  margin: 6px 0;
  font-size: 14px;
}

.contact-box a {
  color: #00ffff;
  text-decoration: none;
}

.contact-box a:hover {
  text-decoration: underline;
}


/*profilepic*/

.profile-pic {
  width: 100px;
  height: 100px;
  border-radius: 50%; /* circular */
  object-fit: cover;
  margin-bottom: 15px;
}



